{
  "name": "postman",
  "version": "0.1.0",
  "description": "An API load-testing tool",
  "author": "Zeann3th",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build -b swc",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.15.6",
    "@nestjs/common": "^11.0.16",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/throttler": "^6.4.0",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "chartjs-node-canvas": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "helmet": "^8.0.0",
    "keytar": "^7.9.0",
    "morgan": "^1.10.0",
    "pdfkit": "^0.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.24",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/pdfkit": "^0.13.9",
    "drizzle-kit": "^0.31.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "bin": "./dist/src/main.js",
  "pkg": {
    "scripts": "./dist/src/**/*.js",
    "assets": [
      "dist/src/modules/flow/flow.worker.js",
      "dist/migrations/**/*",
      "node_modules/axios/**/*",
      "node_modules/drizzle-orm/*.{js,cjs}",
      "node_modules/drizzle-orm/sqlite-core/**/*",
      "node_modules/drizzle-orm/query-builders/**/*",
      "node_modules/drizzle-orm/libsql/**/*",
      "node_modules/@libsql/**/*",
      "node_modules/pdfkit/js/data/Helvetica.afm",
      "node_modules/pdfkit/js/data/Helvetica-Bold.afm"
    ],
    "targets": [
      "node22-win-x64"
    ],
    "outputPath": "./bin"
  }
}